# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Fast Foodz' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Fast Foodz

  # NSLayoutAnchor framework
  # https://github.com/Rightpoint/Anchorage
  pod 'Anchorage', '~> 4.4.0'

  # NSAttributedString framework
  # https://github.com/Rightpoint/bonmot
  pod 'BonMot', '~> 5.5.1'

  # Networking
  # https://github.com/Alamofire/Alamofire
  pod 'Alamofire', '~> 4.8.2'
  # https://github.com/RxSwiftCommunity/RxAlamofire
  pod 'RxAlamofire', '~> 5.0.0'

  # Rx
  # https://github.com/ReactiveX/RxSwift
  pod 'RxSwift', '~> 5.0.0'
  pod 'RxCocoa', '~> 5.0.0'

  # Image Handling
  # https://github.com/SDWebImage/SDWebImage
  pod 'SDWebImage', '~> 5.6.1'

end
